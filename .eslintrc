{
  "env": {
    "browser": true
  },
  "extends": [
    "@inlinedb/inlinedb",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "sort-imports": 0
  }
}
